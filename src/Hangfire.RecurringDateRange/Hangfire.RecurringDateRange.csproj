<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;net452</TargetFrameworks>
    <version>$(Version)</version>
    <title>Hangfire.RecurringDateRange</title>
    <authors>Joe Coutcher,Sergey Odinokov</authors>
    <owners>jcoutch,odinserj</owners>
    <projectUrl>https://github.com/jcoutch/Hangfire.RecurringDateRange</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/jcoutch/Hangfire.RecurringDateRange/blob/master/LICENSE</licenseUrl>
    <description>A fork of Hangfire's recurring background job processing (to add date ranges, and additional cron support)</description>
    <tags>Hangfire OWIN Long-Running Background Fire-And-Forget Delayed Recurring Tasks Jobs Scheduler Threading Queues</tags>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <AssemblyFileVersion>$(Version)</AssemblyFileVersion>
    <VersionPrefix>0.1.0.0</VersionPrefix>
    <VersionSuffix>alpha</VersionSuffix>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Hangfire.Core" Version="1.6.15" />
    <PackageReference Include="NCrontab.Advanced" Version="1.2.3.3" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
</Project>
